/get-post
variables: {
	id
}

{
	id,
	time,
	heartsCount,
	commentsCount,
	contentType, 'IMAGE' | 'TEXT',
	content,
	author: {
		id,
		avatar,
		fullName,
		username
	},
	comments: [{
		author: {
			id,
			avatar,
			fullName,
			username
		},
		content,
		time
	}]
}