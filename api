}

/posts
{
	variables: {
		userId
	}
}
{
	posts: [{
		id,
		time,
		heartsCount,
		commentsCount,
		contentType, 'IMAGE' | 'TEXT',
		content,
		author: {
			id,
			avatar,
			fullName,
			username
		}
	}]
}

